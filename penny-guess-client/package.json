{
  "name": "penny-guess-client",
  "version": "1.0.0",
  "description": "A website for trying things out and annoying Penny",
  "main": "src/main/app.js",
  "private": true,
  "keywords": [],
  "author": "Andrew Barnes",
  "license": "ISC",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./src/test/test_helper.js 'src/test/**/*.@(js)' --recursive",
    "test:watch": "mocha --compilers js:babel-core/register --require ./src/test/test_helper.js 'src/test/**/*.@(js)' --recursive --watch",
    "start": "webpack-dev-server --inline --hot --port 5006",
    "prebuild": "rm -rf ./target/* &&  npm install --no-optional",
    "build": "webpack --mode development",
    "prebuildprod": "rm -rf ./target/* &&  npm install --no-optional",
    "buildprod": "webpack --mode production"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
